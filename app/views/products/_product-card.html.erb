<ul>
  <% @products.each do |product| %>
    <div class="card">
      <li><%= link_to product.name, product_path(product) %></li>
      <p>$<%= product.price %></p>
      <p>Imported From <%= product.country  %></p>
      <% if product.product_photo.attached? %>
        <%= image_tag product.product_photo %>
      <% end %>
    </div>
  <% end %>  
</ul>
